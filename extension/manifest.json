{
  "name": "Gmail StrikeThrough",
  "version": "1.1",
  "description": "Enable StrikeThrough in Gmail or Google Apps Mail.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
